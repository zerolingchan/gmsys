<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neuedu.mapper.DemoMapper">
  <resultMap id="BaseResultMap" type="com.neuedu.model.Demo">
    <result column="ID" jdbcType="INTEGER" property="id" />
    <result column="NAME" jdbcType="VARCHAR" property="name" />
  </resultMap>
  <insert id="insert" keyColumn="ID" keyProperty="id" parameterType="com.neuedu.model.Demo" useGeneratedKeys="true">
    insert into demo (NAME)
    values (#{name,jdbcType=VARCHAR})
  </insert>
  <select id="selectAll" resultMap="BaseResultMap">
    select ID, NAME
    from demo
  </select>
</mapper>